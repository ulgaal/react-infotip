{
  "name": "@ulgaal/react-infotip",
  "version": "1.5.1",
  "description": "A library to provide tooltips and sticky-notes to React apps",
  "keywords": [
    "react",
    "infotip",
    "tooltip",
    "sticky-note",
    "post-it note",
    "speech balloons",
    "bubbles"
  ],
  "homepage": "http://github.com/ulgaal/react-infotip",
  "bugs": {
    "url": "https://github.com/ulgaal/react-infotip/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Ulrich Gaal",
    "email": "react-infotip@coder-tribe.ovh"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "module": "lib/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ulgaal/react-infotip.git"
  },
  "scripts": {
    "build": "babel --out-dir lib src",
    "test": "node config/test.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "doc": "scripts/doc.sh"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "lodash.isequal": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-knobs": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/addon-storysource": "6.2.9",
    "@storybook/addons": "6.2.9",
    "@storybook/react": "6.2.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "core-js": "^3.37.1",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "promisify-node": "^0.5.0",
    "prop-types": "^15.7.2",
    "react": "^18.3.1",
    "react-docgen": "^5.3.1",
    "react-dom": "^18.3.1",
    "react-reducer-table": "^1.3.1",
    "standard": "^17.1.0",
    "storybook-readme": "^5.0.9",
    "svg-react-loader": "^0.4.6"
  },
  "peerDependencies": {
    "prop-types": "^15.x",
    "react": "^16.x || ^17.x || ^18.x",
    "react-dom": "16.x || ^17.x || ^18.x"
  },
  "standard": {
    "globals": [
      "SVGElement",
      "Element"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": "current node"
        }
      ]
    ]
  },
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/config/jest/transformer-babel.js"
    }
  }
}
